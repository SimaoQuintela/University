    <config key="annotations">
        <config key="annotation_0">
            <entry key="text" type="xstring" value="&lt;p&gt;&lt;strong&gt;Finding Association Rules for Market Basket Analysis&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;The aim of a market basket analysis is to analyze whether there are combinations of products that frequently co-occur in transactions. This workflow illustrates how to build a recommendation engine for market basket analysis.&lt;/p&gt;&lt;p&gt;&lt;/p&gt;"/>
            <entry key="contentType" type="xstring" value="text/html"/>
            <entry key="bgcolor" type="xint" value="16777215"/>
            <entry key="x-coordinate" type="xint" value="-15"/>
            <entry key="y-coordinate" type="xint" value="-155"/>
            <entry key="width" type="xint" value="780"/>
            <entry key="height" type="xint" value="115"/>
